---

//  UNCHANGING <HEAD> VARIABLES 
let pageICBM = "39.9526, 75.1652";
let pageGeoPosition = "39.9526;75.1652";
let pageGeoRegion = "US-PA";
let pageGeoPlaceName = "Philadelphia";
// TO DO: Test to make sure this image link works!
let pageImageLink = "https://larrymg.me/images/celtic_harp_vaporwave.jpg";
let pageImageAlt = "Purple outline of a Celtic harp centered within a vaporwave landscape of dark grey rectangles & triangles.";
let pageSiteName = "larrymg.me";
let pageOGLocale = "en_US";
let pageAuthor = "Labhr√°s (Larry) Mac Gabhann";

--- 

    <!-- Control the behavior of search engine crawling and indexing -->
    <meta name="robots" content="index,follow" />
    <!-- All Search Engines -->
    <meta name="googlebot" content="index,follow" />
    <!-- Google Specific -->
    <!-- Tells Google not to show the sitelinks search box -->
    <meta name="google" content="nositelinkssearchbox" />
    <!-- Geo tags -->
    <meta name="ICBM" content={pageICBM} />
    <meta name="geo.position" content={pageGeoPosition} />
    <meta name="geo.region" content={pageGeoRegion} />
    <meta name="geo.placename" content={pageGeoPlaceName} />
    <!-- Facebook Open Graph -->
    <meta property="og:type" content="website" />
    <meta
      property="og:image"
      content={pageImageLink}
    />
    <meta
      property="og:image:alt"
      content={pageImageAlt}
    />
    <meta
      property="og:site_name"
      content={pageSiteName}
    />
    <meta property="og:locale" content={pageOGLocale} />
    <meta property="article:author" content={pageAuthor} />
    <!-- Styling & Fonts -->
    <link rel="preload" href="/src/scss/main.scss" as="style" />
    <link rel="preload" href="../../assets/fonts/KulimPark-Regular.ttf" as="font"
    type="font/ttf" crossorigin /> <link rel="preload"
    href="../../assets/fonts/KulimPark-Bold.ttf" as="font" type="font/ttf" crossorigin
    /> <link rel="preload" href="../../assets/fonts/KulimPark-Italic.ttf" as="font"
    type="font/ttf" crossorigin /> <link rel="preload"
    href="../../assets/fonts/KulimPark-SemiBoldItalic.ttf" as="font" type="font/ttf"
    crossorigin />
    <link rel="stylesheet" href="/src/scss/main.scss" />
    <!-- Favicon Settings 
            Generated using: https://realfavicongenerator.net -->
    <link rel="icon" type="image/x-icon" href="../../public/favicon.ico" />
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="theme-color" content="#ffffff" />
   <link rel="apple-touch-icon" sizes="60x60" href="../../assets/apple-touch-icon.png/" />
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="../../assets/favicon-32x32.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="../../assets/favicon-16x16.png"
    />
    <link rel="manifest" href="../../assets/site.webmanifest" />
    <link rel="mask-icon" href="../../assets/safari-pinned-tab.svg" color="#5bbad5" />
